{
  "name": "dina-collections-ui",
  "version": "0.1.0",
  "private": true,
  "license": "AGPL V3",
  "contributors": [
    "Anton Ã–berg <oberg.anton@gmail.com>",
    "Fredrik Olovsson <fogithub@gmail.com>"
  ],
  "proxy": {
    "/auth": {
      "target": "https://alpha-cm.dina-web.net",
      "xfwd": true,
      "changeOrigin": true
    },
    "/taxonomy": {
      "changeOrigin": true,
      "pathRewrite": {
        "^/taxonomy": ""
      },
      "target": "https://alpha-slimtaxonomy.dina-web.net",
      "xfwd": true
    },
    "/collections": {
      "target": "https://alpha-api.dina-web.net/",
      "xfwd": true,
      "changeOrigin": true
    }
  },
  "dependencies": {
    "ajv": "^5.2.3",
    "babel-cli": "^6.26.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-inline-json-import": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-export-extensions": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "body-parser": "^1.18.2",
    "classnames": "^2.2.5",
    "debug": "^3.1.0",
    "dina-schema": "https://github.com/DINA-Web/dina-schema.git#master",
    "dns": "file:./src/mock/dns",
    "express": "^4.16.2",
    "glob": "^7.1.2",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "json-schema-faker": "^0.4.4",
    "keycloak-connect": "^3.4.1-cr.1",
    "lodash.debounce": "^4.0.8",
    "lodash.throttle": "^4.1.1",
    "markdown-it": "^8.4.0",
    "object-path": "^0.11.4",
    "object-path-immutable": "^0.5.2",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "pm2": "^2.8.0",
    "pre-push": "^0.1.1",
    "prettier": "^1.7.2",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-transition-group": "^15.6.2",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "1.0.17",
    "reactstrap": "^4.8.0",
    "redux": "^3.7.2",
    "redux-form": "^7.0.4",
    "redux-form-with-ajv": "0.3.0",
    "redux-logger": "^3.0.6",
    "redux-module-local-storage": "^1.1.0",
    "reselect": "^3.0.1",
    "response-time": "^2.3.2",
    "semantic-ui": "https://github.com/DINA-Web/dina-semantic-ui.git#master",
    "semantic-ui-react": "^0.74.2",
    "sequelize": "^4.25.1",
    "sequelize-cli": "^3.2.0",
    "uuid": "^3.1.0",
    "watch-glob": "^0.1.3",
    "whatwg-fetch": "^2.0.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "build": "npm run build-css && npm run markdown:build && REACT_APP_ENABLE_ENDPOINT_MOCKS=false react-scripts build",
    "build-css": "node-less-chokidar src/ -o src/",
    "build-snippets": "node ./src/scripts/snippets/index.js",
    "build-storybook": "npm run update-storybook-imports && build-storybook -c .storybook -o build-storybook",
    "build:mock": "npm run build-css && npm run markdown:build && REACT_APP_ENABLE_ENDPOINT_MOCKS=true react-scripts build",
    "eject": "react-scripts eject",
    "ensure-no-diff": "git diff --exit-code",
    "lint:js": "eslint 'src/**/*.js'",
    "lint:js:fix": "eslint 'src/**/*.js' --fix",
    "markdown:build": "node src/scripts/markdown/buildRenderedMarkdownFiles.js",
    "markdown:watch": "node src/scripts/markdown/watchMarkdownFiles.js",
    "mount:admin": "MOUNT_APP=admin node ./src/scripts/app/mountApp.js",
    "mount:collectionsUi": "MOUNT_APP=collectionsUi node ./src/scripts/app/mountApp.js",
    "pretty": "prettier --single-quote --write 'src/**/*.{js,jsx}'",
    "start": "NODE_PATH=./src/ npm-run-all -p watch-css markdown:watch start-js",
    "start-js": "react-scripts start",
    "storybook": "npm run update-storybook-imports && start-storybook -p 9009 -s public",
    "test": "npm run update-storybook-imports && npm run lint:js && react-scripts test --env=jsdom",
    "test:api": "API_TEST=true react-scripts test src/**/*.api.test.js",
    "test:api:watch": "API_TEST=true react-scripts test src/**/*.api.test.js --watch",
    "test:coverage": "npm test -- --coverage",
    "test:unit:watch": "react-scripts test --env=jsdom --watch",
    "update-git-dependencies": "yarn upgrade dina-schema && yarn upgrade semantic-ui",
    "update-storybook-imports": "node ./src/scripts/storybook/createStorybookImports",
    "watch-css": "npm run build-css && node-less-chokidar src/ -o src/ --watch --recursive"
  },
  "pre-push": [
    "update-git-dependencies",
    "ensure-no-diff"
  ],
  "devDependencies": {
    "@storybook/addon-actions": "3.2.12",
    "@storybook/addon-comments": "3.2.12",
    "@storybook/addon-info": "3.2.12",
    "@storybook/addon-knobs": "3.2.12",
    "@storybook/addon-links": "3.2.12",
    "@storybook/addon-storyshots": "3.2.12",
    "@storybook/react": "3.2.12",
    "babel-eslint": "^8.0.1",
    "deep-freeze": "^0.0.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^7.4.0",
    "jest-localstorage-mock": "^2.0.1",
    "node-less-chokidar": "^0.1.2",
    "npm-run-all": "^4.1.1",
    "raw-loader": "^0.5.1",
    "react-test-renderer": "^16.0.0",
    "redux-mock-store": "^1.3.0",
    "source-map-explorer": "^1.5.0",
    "storybook-readme": "3.0.6"
  },
  "engines": {
    "node": "8.9.1",
    "yarn": "^1.3.2"
  }
}
